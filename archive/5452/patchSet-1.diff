From 57a6e98293925f109b2028c6aa56b6700f2b4e2f Mon Sep 17 00:00:00 2001
From: Robert Mustacchi <rm@joyent.com>
Date: Tue, 7 Mar 2017 18:51:05 +0000
Subject: [PATCH] OS-7549 Sync CTF review changes back

---
 Makefile              | 2 +-
 pkgsrc/onbld/packlist | 1 -
 src/Makefile.defs     | 2 +-
 3 files changed, 2 insertions(+), 3 deletions(-)

diff --git a/Makefile b/Makefile
index 2fd6b1ca..d4374038 100644
--- a/Makefile
+++ b/Makefile
@@ -74,7 +74,7 @@ CTFBINDIR = \
 	$(ROOT)/projects/illumos/usr/src/tools/proto/*/opt/onbld/bin/i386
 CTFMERGE =	$(CTFBINDIR)/ctfmerge
 CTFCONVERT =	$(CTFBINDIR)/ctfconvert
-ALTCTFCONVERT =	$(CTFBINDIR)/ctfconvert-altexec
+ALTCTFCONVERT =	$(CTFBINDIR)/ctfconvert
 
 SUBDIR_DEFS = \
 	CTFMERGE=$(CTFMERGE) \
diff --git a/pkgsrc/onbld/packlist b/pkgsrc/onbld/packlist
index 12be06c0..eaf4d9b3 100644
--- a/pkgsrc/onbld/packlist
+++ b/pkgsrc/onbld/packlist
@@ -21,7 +21,6 @@
 ./bin/i386/ctfdiff
 ./bin/i386/ctfdump
 ./bin/i386/ctfmerge
-./bin/i386/ctfmerge-altexec
 ./bin/i386/ctfstabs
 ./bin/i386/ctfstrip
 ./bin/i386/cw
diff --git a/src/Makefile.defs b/src/Makefile.defs
index 8ed43227..4b3559e1 100644
--- a/src/Makefile.defs
+++ b/src/Makefile.defs
@@ -42,7 +42,7 @@ GXX =		$(NATIVEDIR)/usr/bin/g++
 CTFBINDIR =	$(ILLUMOS_SRC)/tools/proto/root_i386-nd/opt/onbld/bin/i386
 CTFMERGE =	$(CTFBINDIR)/ctfmerge
 CTFCONVERT =	$(CTFBINDIR)/ctfconvert
-ALTCTFCONV =	$(CTFBINDIR)/ctfconvert-altexec
+ALTCTFCONV =	$(CTFBINDIR)/ctfconvert
 
 SYSINCDIRS =	/usr/include
 SYSLIBDIRS =	/usr/lib /lib
-- 
2.21.0

