diff --git a/lib/cloudapi2.js b/lib/cloudapi2.js
index 4a5a9a5..3a82037 100644
--- a/lib/cloudapi2.js
+++ b/lib/cloudapi2.js
@@ -170,7 +170,6 @@ CloudApi.prototype._getAuthHeaders =
     assert.string(method, 'method');
     assert.string(path, 'path');
     assert.func(callback, 'callback');
-    var self = this;
 
     var headers = {};
 
diff --git a/lib/common.js b/lib/common.js
index 3c259ae..a0865c8 100644
--- a/lib/common.js
+++ b/lib/common.js
@@ -734,7 +734,7 @@ function promptPassphraseUnlockKey(opts, cb) {
         }
 
         cb(null);
-    }
+    };
 
     getpass.getPass(getpassOpts, tryPass);
 }
diff --git a/lib/tritonapi.js b/lib/tritonapi.js
index 36868a9..75607dc 100644
--- a/lib/tritonapi.js
+++ b/lib/tritonapi.js
@@ -259,7 +259,9 @@ TritonApi.prototype._setupProfile = function _setupProfile(cb) {
 
     if (profile.privKey) {
         var key = sshpk.parsePrivateKey(profile.privKey);
-        this.keyPair = opts.principal.keyPair = auth.KeyPair.fromPrivateKey(key);
+        this.keyPair =
+            opts.principal.keyPair =
+            auth.KeyPair.fromPrivateKey(key);
         this.cloudapi = cloudapi.createClient(opts);
         cb(null);
     } else {
