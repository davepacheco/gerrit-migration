From f347fc7817a3ba43fdb8376ab037d2c56ea2a21b Mon Sep 17 00:00:00 2001
From: Brittany Wald <brittany.wald@joyent.com>
Date: Thu, 7 Sep 2017 22:53:33 +0000
Subject: [PATCH] MANTA-3413 node-fash seems to intend add-data behavior it
 does not enable

---
 bin/fash.js | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/bin/fash.js b/bin/fash.js
index ea04d9c..607cc17 100755
--- a/bin/fash.js
+++ b/bin/fash.js
@@ -210,7 +210,7 @@ Fash.prototype.do_deserialize_ring.help = (
 Fash.prototype.do_add_data = function (subcmd, opts, args, callback) {
     var self = this;
 
-    if (opts.help || args.length !== 0 || !opts.v || !opts.d || !opts.b) {
+    if (opts.help || args.length !== 0 || !opts.v || !opts.b) {
         this.do_help('help', {}, [subcmd], function (err) {
             return callback(err ? err : true);
         });
