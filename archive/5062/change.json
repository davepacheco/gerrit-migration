g\"."},{"file":"lib/validations.js","line":163,"reviewer":{"name":"Pedro Palazón Candel","email":"pedro@joyent.com","username":"kusor"},"message":"Cannot see why not. Added, better to fail early if possible."},{"file":"lib/validations.js","line":233,"reviewer":{"name":"Mike Gerdts","email":"mike.gerdts@joyent.com","username":"mgerdts"},"message":"!pkg.flexible_disk"},{"file":"lib/validations.js","line":233,"reviewer":{"name":"Pedro Palazón Candel","email":"pedro@joyent.com","username":"kusor"},"message":"Done"},{"file":"package.json","line":4,"reviewer":{"name":"Mike Gerdts","email":"mike.gerdts@joyent.com","username":"mgerdts"},"message":"0"},{"file":"package.json","line":4,"reviewer":{"name":"Pedro Palazón Candel","email":"pedro@joyent.com","username":"kusor"},"message":"Good call!"},{"file":"test/api.test.js","line":434,"reviewer":{"name":"Marsell Kukuljevic","email":"marsell@joyent.com","username":"marsell"},"message":"Layout is different from the rest of the array."},{"file":"test/api.test.js","line":434,"reviewer":{"name":"Pedro Palazón Candel","email":"pedro@joyent.com","username":"kusor"},"message":"I think it should be like this for the rest of the array but haven\u0027t changed to do not make more noise than help. Anyway, modifying now. If possible we should open and close scope brackets at the same indentation level, IMO"}],"files":[{"file":"/COMMIT_MSG","type":"ADDED","insertions":7,"deletions":0},{"file":"etc/config.test.json","type":"MODIFIED","insertions":8,"deletions":0},{"file":"lib/validations.js","type":"MODIFIED","insertions":26,"deletions":-4},{"file":"package.json","type":"MODIFIED","insertions":1,"deletions":-1},{"file":"sapi_manifests/papi/template","type":"MODIFIED","insertions":8,"deletions":0},{"file":"test/api.test.js","type":"MODIFIED","insertions":28,"deletions":-4}],"sizeInsertions":71,"sizeDeletions":-9},{"number":"3","revision":"9e6d90f39bf9203322324c42cde3b44f392162be","parents":["e684de6a71a96e0ad0522dd7257ba141650f5601"],"ref":"re