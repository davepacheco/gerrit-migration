From 595814295b75e9cb5b0e6d774060e0910d6e376e Mon Sep 17 00:00:00 2001
From: Jared Morrow <jm@joyent.com>
Date: Tue, 31 Oct 2017 16:18:45 -0600
Subject: [PATCH] MANTA-3484 Binder's ZK timeout setting should be
 sessionTimeout with node-zkstream

---
 lib/zk.js | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/lib/zk.js b/lib/zk.js
index f1fd475..5982c3b 100644
--- a/lib/zk.js
+++ b/lib/zk.js
@@ -26,7 +26,7 @@ function ZKCache(options) {
                 address: process.env.ZK_HOST || '127.0.0.1',
                 port: 2181,
                 log: options.log,
-                timeout: 30000
+                sessionTimeout: 30000
         });
         this.ca_domain = options.domain;
         this.ca_log = options.log;
