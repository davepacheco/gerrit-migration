Subject: [PATCH] TRITON-310 we should have a list of all triton-y repos

---
 repos.json | 57 ++++++++++++++++++++++++++++++++++++++++++++++++++++++
 1 file changed, 57 insertions(+)
 create mode 100644 repos.json

diff --git a/repos.json b/repos.json
new file mode 100644
index 0000000..b79aa15
--- /dev/null
+++ b/repos.json
@@ -0,0 +1,57 @@
+{
+    "jrVersion": 1,
+    "description": "Triton Data Center public repositories",
+
+    "defaults": {
+        "labels": {
+            "triton": true,
+            "public": true,
+            "state": "active"
+        }
+    },
+
+    "repositories": [
+        {
+            "name": "triton"
+        },
+        {
+            "name": "sdc-imgapi",
+            "labels": {
+                "tritonservice": "imgapi",
+                "vm": true
+            }
+        },
+        {
+            "name": "sdc-vmapi",
+            "labels": {
+                "tritonservice": "vmapi",
+                "vm": true
+            }
+        },
+        {
+            "name": "mahi",
+            "labels": {
+                "tritonservice": "mahi",
+                "vm": true
+            }
+        },
+        {
+            "name": "rfd",
+            "labels": {
+                "meta": true
+            }
+        },
+        {
+            "name": "node-task-agent",
+            "labels": {
+                "state": "deprecated"
+            }
+        }
+    ],
+
+    "notRepositories": [
+        {
+            "name": "manta"
+        }
+    ]
+}
