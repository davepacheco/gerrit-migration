Subject: [PATCH] TRITON-462 triton services still on nodejs v4 should use the
 latest point release

---
 Makefile | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/Makefile b/Makefile
index f525f5f..7d1c33e 100644
--- a/Makefile
+++ b/Makefile
@@ -5,7 +5,7 @@
 #
 
 #
-# Copyright (c) 2017, Joyent, Inc.
+# Copyright (c) 2018, Joyent, Inc.
 #
 
 #
@@ -26,7 +26,7 @@ JSSTYLE_FLAGS	 = -f tools/jsstyle.conf
 SERVICE_NAME     = volapi
 SMF_MANIFESTS_IN = smf/manifests/$(SERVICE_NAME)-server.xml.in smf/manifests/$(SERVICE_NAME)-updater.xml.in
 
-NODE_PREBUILT_VERSION=v4.6.1
+NODE_PREBUILT_VERSION=v4.9.0
 
 ifeq ($(shell uname -s),SunOS)
 	NODE_PREBUILT_TAG=zone
@@ -116,4 +116,4 @@ test-integration-in-coal:
 	@ssh root@coal 'LOG_LEVEL=$(LOG_LEVEL) /zones/$$(vmadm lookup -1 alias=volapi0)/root/opt/smartdc/volapi/test/runtests $(TEST_ARGS)'
 
 .PHONY: test
-test: test-integration-in-coal
\ No newline at end of file
+test: test-integration-in-coal
