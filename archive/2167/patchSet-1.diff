From 74a561727aad715f733cfed6fe5b9a7d7d51c485 Mon Sep 17 00:00:00 2001
From: Cody Peter Mello <cody.mello@joyent.com>
Date: Thu, 29 Jun 2017 01:43:47 +0000
Subject: [PATCH] MANTA-3336 Pull Muskie dependencies over HTTPS instead of SSH

---
 .gitmodules  |  8 ++++----
 package.json | 10 +++++-----
 2 files changed, 9 insertions(+), 9 deletions(-)

diff --git a/.gitmodules b/.gitmodules
index 6dbb13c..721ad95 100644
--- a/.gitmodules
+++ b/.gitmodules
@@ -1,12 +1,12 @@
 [submodule "deps/jsstyle"]
 	path = deps/jsstyle
-	url = git://github.com/davepacheco/jsstyle.git
+	url = https://github.com/davepacheco/jsstyle.git
 [submodule "deps/restdown"]
 	path = deps/restdown
-	url = git://github.com/trentm/restdown.git
+	url = https://github.com/trentm/restdown.git
 [submodule "deps/javascriptlint"]
 	path = deps/javascriptlint
-	url = git://github.com/davepacheco/javascriptlint.git
+	url = https://github.com/davepacheco/javascriptlint.git
 [submodule "deps/manta-scripts"]
 	path = deps/manta-scripts
-	url = git@github.com:joyent/manta-scripts.git
+	url = https://github.com/joyent/manta-scripts.git
diff --git a/package.json b/package.json
index 4b3a25d..9e81a28 100644
--- a/package.json
+++ b/package.json
@@ -9,7 +9,7 @@
         "url": "git+ssh://git@github.com:joyent/muskie.git"
     },
     "dependencies": {
-        "aperture-config": "git+ssh://git@github.com:joyent/aperture-config.git#master",
+        "aperture-config": "git+https://github.com/joyent/aperture-config.git#master",
         "assert-plus": "0.1.5",
         "backoff": "2.3.0",
         "bunyan": "0.22.1",
@@ -22,13 +22,13 @@
         "jsprim": "^1.3.1",
         "kang": "1.1.0",
         "keep-alive-agent": "0.0.1",
-        "keyapi": "git+ssh://git@github.com:joyent/keyapi.git#c30dd2710ad2175095dc0e96479686fa774b8063",
-        "libmanta": "git+ssh://git@github.com:joyent/node-libmanta.git#master",
+        "keyapi": "git+https://github.com/joyent/keyapi.git#e14b3d58",
+        "libmanta": "git+https://github.com/joyent/node-libmanta.git#master",
         "libuuid": "0.1.2",
         "lru-cache": "2.3.1",
         "lstream": "0.0.4",
         "mahi": "2.0.1",
-        "marlin": "git+ssh://git@github.com:joyent/manta-marlin.git#master",
+        "marlin": "git+https://github.com/joyent/manta-marlin.git#master",
         "mime": "1.2.11",
         "moray": "3.1.1",
         "once": "1.3.0",
@@ -39,7 +39,7 @@
         "xtend": "2.1.1"
     },
     "devDependencies": {
-        "smartdc": "git+ssh://git@github.com:joyent/node-smartdc.git#v7.3",
+        "smartdc": "7.3.1",
         "manta": "4.3.0",
         "nodeunit": "0.9.1",
         "node-uuid": "1.4.1"
