+
 ### API
 
     var data = {
         email: 'mark.cavage@joyent.com',
         ismanager: false
     };
-    client.putObject('foo', 'mcavage', data, function (err) {
+    client.putObject('foo', 'mcavage', data, function (err, res) {
         assert.ifError(err);
+        console.log(res.etag);
     });
 
