Subject: [PATCH] TRITON-1358 Logarchiver logset manta path is incorrect

---
 etc/logsets.json | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/etc/logsets.json b/etc/logsets.json
index ea35280..8162a93 100644
--- a/etc/logsets.json
+++ b/etc/logsets.json
@@ -3,7 +3,7 @@
     "name": "firewall_logs",
     "search_dirs": [ "/var/log/firewall" ],
     "regex": "^/var/log/firewall/([0-9a-f]{8}\\-[0-9a-f]{4}\\-[0-9a-f]{4}\\-[0-9a-f]{4}\\-[0-9a-f]{12})/([0-9a-f]{8}\\-[0-9a-f]{4}\\-[0-9a-f]{4}\\-[0-9a-f]{4}\\-[0-9a-f]{12})/([0-9]+)-([0-9]+)-([0-9]+)T([0-9]+):([0-9]+):([0-9]+)\\.log.gz$",
-    "manta_path": "/%U/reports/firewall-logs/$2/#y/#m/#d/#y-%m-%dT%H:%M:%S.log.gz",
+    "manta_path": "/%U/reports/firewall-logs/$2/#y/#m/#d/#y-#m-#dT#H:#M:#S.log.gz",
     "customer_uuid": "$1",
     "date_string": {
       "y": "$3", "m": "$4", "d": "$5",
