From b378af65e75b0d353a83ca7fa5c1ef134e967707 Mon Sep 17 00:00:00 2001
From: Chris Burroughs <chris.burroughs@joyent.com>
Date: Fri, 19 Jan 2018 17:06:00 -0500
Subject: [PATCH] HEAD-2385 dhcpd config should use auto.ADMIN_IP instead of
 dhcpd_admin_ip

---
 sapi_manifests/dhcpd/template | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/sapi_manifests/dhcpd/template b/sapi_manifests/dhcpd/template
index 3cd9c67..bc86c39 100644
--- a/sapi_manifests/dhcpd/template
+++ b/sapi_manifests/dhcpd/template
@@ -29,7 +29,7 @@
   "port": 67,
   "tftpRoot": "/tftpboot",
   "defaultGateway": "{{{compute_node_default_gateway}}}",
-  "serverIp": "{{{dhcpd_admin_ip}}}",
+  "serverIp": "{{{auto.ADMIN_IP}}}",
   "leaseTime": {{{dhcp_lease_time}}},
   "resolvers": {{{resolvers}}}{{^resolvers}}[]{{/resolvers}},
   "netmask": "{{{admin_netmask}}}",
