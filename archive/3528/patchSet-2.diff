Subject: [PATCH] TRITON-200 sdc-docker cli-nfs-shared-volumes-disabled.test.js
 is flaky

---
 test/integration/cli-nfs-shared-volumes-disabled.test.js | 6 +++---
 1 file changed, 3 insertions(+), 3 deletions(-)

diff --git a/test/integration/cli-nfs-shared-volumes-disabled.test.js b/test/integration/cli-nfs-shared-volumes-disabled.test.js
index 71df3b6..b832585 100644
--- a/test/integration/cli-nfs-shared-volumes-disabled.test.js
+++ b/test/integration/cli-nfs-shared-volumes-disabled.test.js
@@ -34,7 +34,7 @@ var NFS_SHARED_VOLUME_NAMES_PREFIX =
     testVolumes.getNfsSharedVolumesNamePrefix();
 var SAPI_CLIENT;
 var SAPI_APP;
-var STATE_RETRIES = 120;
+var STATE_RETRIES = 180;
 
 // wait for /admin/config to have state either 'enabled' or 'disabled' for
 // experimental_docker_nfs_shared_volumes
@@ -137,8 +137,8 @@ test('setup', function (tt) {
                         + app.metadata.experimental_docker_nfs_shared_volumes);
                 }
 
-                if (app.metadata.experimental_docker_nfs_shared_volumes ===
-                    false) {
+                if (app.metadata.experimental_docker_nfs_shared_volumes !==
+                    true) {
                     t.comment('NFS volumes support already disabled, no need '
                         + 'to turn it off');
                     t.end();
