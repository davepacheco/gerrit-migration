 .gitmodules  | 2 +-
 deps/jsstyle | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/.gitmodules b/.gitmodules
index eb36562..a295d6e 100644
--- a/.gitmodules
+++ b/.gitmodules
@@ -3,7 +3,7 @@
 	url = https://github.com/davepacheco/javascriptlint.git
 [submodule "deps/jsstyle"]
 	path = deps/jsstyle
-	url = https://github.com/davepacheco/jsstyle.git
+	url = https://github.com/joyent/jsstyle.git
 [submodule "deps/jsdoc3"]
 	path = deps/jsdoc3
 	url = https://github.com/jsdoc3/jsdoc.git
diff --git a/deps/jsstyle b/deps/jsstyle
index 2e7616f..52dc973 160000
--- a/deps/jsstyle
+++ b/deps/jsstyle
@@ -1 +1 @@
-Subproject commit 2e7616f0730f0bfbd05994d60366ad5acff1e88f
+Subproject commit 52dc973cf64da11834eca7cf46ebce8518e3ee88
