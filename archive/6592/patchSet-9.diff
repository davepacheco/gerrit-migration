-	snaps=$(ls -1 $ZONEPATH/$SNAPSHOT_DIR)
-
-	for snap in ${snaps}; do
-		snapdir=$ZONEPATH/$SNAPSHOT_DIR/$(echo ${snap} | sed -e "s/^vmsnap-//")
-		umount ${snapdir}
-		rmdir ${snapdir}
