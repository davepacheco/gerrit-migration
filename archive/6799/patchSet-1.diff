Subject: [PATCH] TRITON-1872 Incorrect location echoed in
 releng/ls-missing-release-builds script

---
 tools/releng/ls-missing-release-builds | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/tools/releng/ls-missing-release-builds b/tools/releng/ls-missing-release-builds
index 5b2f7f5..6321109 100755
--- a/tools/releng/ls-missing-release-builds
+++ b/tools/releng/ls-missing-release-builds
@@ -113,6 +113,6 @@ echo "$targets" | while read target buildisprivate; do
     latest=$(mls $latest_mpath 2>/dev/null || true)
     if [[ -z "$latest" ]]; then
         echo "# $target, '$latest_mpath' does not exist" >&2
-        echo "./tools/launch-build ${PLAT_FLAVOR_ARG} -b $release $target"
+        echo "./tools/releng/launch-build ${PLAT_FLAVOR_ARG} -b $release $target"
     fi
 done
