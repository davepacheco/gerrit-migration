From 7f8f036655de84a906c600e43cf6fde00224218c Mon Sep 17 00:00:00 2001
From: Trent Mick <trentm@gmail.com>
Date: Thu, 12 Sep 2019 13:30:17 -0700
Subject: [PATCH] TOOLS-2254 add redirects from mo-docs to modocs.joyent.us
 (restdown files should now always be .md)

---
 Makefile                          | 2 +-
 docs/{index.restdown => index.md} | 0
 2 files changed, 1 insertion(+), 1 deletion(-)
 rename docs/{index.restdown => index.md} (100%)

diff --git a/Makefile b/Makefile
index bdb7a8e..88e27d6 100644
--- a/Makefile
+++ b/Makefile
@@ -16,7 +16,7 @@ TEST_FILTER ?= .*
 #
 # Files
 #
-DOC_FILES	 = index.restdown
+DOC_FILES	 = index.md
 JS_FILES	:= $(shell find lib test -name '*.js')
 JS_FILES	+= $(shell find bin -type f -not -name '.*.swp')
 JSL_CONF_NODE	 = tools/jsl.node.conf
diff --git a/docs/index.restdown b/docs/index.md
similarity index 100%
rename from docs/index.restdown
rename to docs/index.md
