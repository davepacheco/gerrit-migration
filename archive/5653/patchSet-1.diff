From 68b3b7b926004a0b85f12a1223cec646ffab9743 Mon Sep 17 00:00:00 2001
From: Jan Wyszynski <jan.wyszynski@joyent.com>
Date: Wed, 27 Feb 2019 21:55:38 +0000
Subject: [PATCH] MANTA-4135 garbage-collector doesn't handle empty
 GC_ASSIGNED_SHARDS

---
 sapi_manifests/manta-garbage-collector/template | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/sapi_manifests/manta-garbage-collector/template b/sapi_manifests/manta-garbage-collector/template
index e666713..c62c152 100644
--- a/sapi_manifests/manta-garbage-collector/template
+++ b/sapi_manifests/manta-garbage-collector/template
@@ -29,10 +29,10 @@
 		{
 			"host": "{{host}}"
 		}{{^last}},{{/last}}{{/GC_ASSIGNED_SHARDS}}
-		{{^GC_ASSIGNED_SHARDS}}
+		{{^GC_ASSIGNED_SHARDS}}{{#GC_SHARD_NUM_LO}}{{#GC_SHARD_NUM_HI}}
 		{{GC_SHARD_NUM_LO}},
 		{{GC_SHARD_NUM_HI}}
-		{{/GC_ASSIGNED_SHARDS}}
+		{{/GC_SHARD_NUM_HI}}{{/GC_SHARD_NUM_LO}}{{/GC_ASSIGNED_SHARDS}}
 	],
 	"buckets": [{{#GC_ASSIGNED_BUCKETS}}
 		{
-- 
2.21.0

