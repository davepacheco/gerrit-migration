-.sp
 .LP
 \fBcv_timedwait\fR(9F), \fBddi_intr_get_pri\fR(9F),
 \fBddi_periodic_delete\fR(9F), \fBddi_intr_get_softint_pri\fR(9F),
 \fBqtimeout\fR(9F), \fBquntimeout\fR(9F), \fBtimeout\fR(9F), \fBuntimeout\fR(9F)
 .SH NOTES
-.sp
 .LP
