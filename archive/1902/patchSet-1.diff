 1 file changed, 6 insertions(+), 1 deletion(-)

diff --git a/lib/consumer.js b/lib/consumer.js
index f135a09..0bc6f21 100644
--- a/lib/consumer.js
+++ b/lib/consumer.js
@@ -304,7 +304,12 @@ function logReq(opts, callback) {
 
     var cnUuid = opts.cnUuid;
     var filter = fmt('cn_uuid=%s', cnUuid);
