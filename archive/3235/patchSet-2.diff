From cc1346a97e8866f39128fee430be9a7fdb7d6c5d Mon Sep 17 00:00:00 2001
From: Josh Wilsdon <josh@wilsdon.ca>
Date: Thu, 18 Jan 2018 14:23:08 -0800
Subject: [PATCH] TRITON-64 index filenames should be index.platform.js instead
 of platform.index.js

---
 lib/index.js                           | 2 +-
 lib/{sunos.index.js => index.sunos.js} | 0
 package.json                           | 2 +-
 3 files changed, 2 insertions(+), 2 deletions(-)
 rename lib/{sunos.index.js => index.sunos.js} (100%)

diff --git a/lib/index.js b/lib/index.js
index cb09e81..5b503f7 100644
--- a/lib/index.js
+++ b/lib/index.js
@@ -9,4 +9,4 @@
  */
 
 var os = require('os');
-module.exports = require('./' + os.platform() + '.index');
+module.exports = require('./index.' + os.platform());
diff --git a/lib/sunos.index.js b/lib/index.sunos.js
similarity index 100%
rename from lib/sunos.index.js
rename to lib/index.sunos.js
diff --git a/package.json b/package.json
index 46a26b2..1f4551c 100644
--- a/package.json
+++ b/package.json
@@ -1,6 +1,6 @@
 {
   "name": "vmadm",
-  "version": "1.0.1",
+  "version": "1.0.2",
   "description": "Node.js wrapper around SmartOS vmadm(1M)",
   "main": "lib/index.js",
   "scripts": {
-- 
